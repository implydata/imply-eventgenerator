      "_comment": "Database traffic - Data: Query execution with flow record",
      "emitter": "vpc_flow_log",
      "variables_on_entry": [{"name": "var_start", "type": "clock"}],
      "delay": {"type": "uniform", "min": 1.0, "max": 3.0},
      "variables": [
        {
          "name": "var_interface_id",
          "type": "enum",
          "values": [
            "eni-0a1b2c3d4e5f60001",
            "eni-0a1b2c3d4e5f60002",
            "eni-0a1b2c3d4e5f60003",
            "eni-0a1b2c3d4e5f60004",
            "eni-0a1b2c3d4e5f60005",
            "eni-0a1b2c3d4e5f60006"
          ],
          "cardinality_distribution": {"type": "exponential", "mean": 2},
          "_comment": "ENI selected per flow record (load balancing)"
        },
        {
          "name": "var_packets",
          "type": "int",
          "cardinality": 0,
          "distribution": {"type": "uniform", "min": 10, "max": 200}
        },
        {
          "name": "var_bytes",
          "type": "int",
          "cardinality": 0,
          "distribution": {"type": "uniform", "min": 1000, "max": 100000}
        },
        {"name": "var_end", "type": "clock"}
      ],
      "transitions": [{"next": "database_traffic_continue", "probability": 0.6}, {"next": "database_traffic_fin", "probability": 0.4}]
    },
    {
      "name": "database_traffic_continue",
